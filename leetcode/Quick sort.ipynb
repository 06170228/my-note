    "def quick_sort(list): #extra-place\n",
    "    smaller = []\n",
    "    bigger = []\n",
    "    keylist = []\n", #
    "    if len(list) <= 1:\n",
    "        return list\n",
    "    else:\n",
    "        key = list[0] #第一個數為key值\n",
    "        for i in list:\n",
    "            if i < key: #比key值小的數\n",
    "                smaller.append(i)\n",
    "            elif i > key: #比key值大的數\n",
    "                bigger.append(i)\n",
    "            else:\n",
    "                keylist.append(i)\n",
    "    smaller = quick_sort(smaller)\n",
    "    bigger = quick_sort(bigger)\n",
    "    return smaller + keylist + bigger"
   ]
